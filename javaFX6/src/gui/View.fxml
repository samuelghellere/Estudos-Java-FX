<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="50.0" layoutY="48.0" onAction="#onComboBoxPersonAction" prefWidth="150.0" promptText="Select person" />
      <ButtonBar layoutX="180.0" layoutY="41.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="btAll" mnemonicParsing="false" onAction="#onBtAllAction" text="All" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
